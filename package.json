{
  "name": "@gen-x/core",
  "version": "0.1.0",
  "description": "Anything to anything",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run clean && npm run generate:genXType",
    "build": "tsc",
    "postbuild": "npm run clean:tests && npm run clean:scripts",
    "clean": "rimraf dist",
    "clean:tests": "rimraf 'dist/**/?(__tests__|__mocks__)'",
    "clean:scripts": "rimraf dist/scripts",
    "generate:genXType": "ts-node src/scripts/generateGenXTypings",
    "prepublishOnly": "npm run build",
    "prestart": "npm run generate:genXType",
    "start": "tsc --watch --preserveWatchOutput",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johngeorgewright/caterpillar.git"
  },
  "keywords": [
    "pipe",
    "promise",
    "generator",
    "stream"
  ],
  "author": "John Wright <johngeorgewright@gmmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johngeorgewright/caterpillar/issues"
  },
  "homepage": "https://github.com/johngeorgewright/caterpillar#readme",
  "devDependencies": {
    "@types/ejs": "^3.0.4",
    "@types/jest": "^26.0.4",
    "@types/lodash.range": "^3.2.6",
    "ejs": "^3.1.3",
    "jest": "^26.1.0",
    "lodash.range": "^3.2.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "web-streams-polyfill": "^2.1.1"
  }
}
