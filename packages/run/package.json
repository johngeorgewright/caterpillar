{
  "name": "@gen-x/run",
  "version": "1.0.2",
  "description": "run a gen-x pipeline",
  "main": "dist/run.js",
  "scripts": {
    "build": "yarn clean && tsc && yarn clean:tests",
    "clean": "rimraf dist",
    "clean:tests": "rimraf 'dist/**/?(__tests__|__mocks__|__setup__)'",
    "release": "yarn semantic-release -e semantic-release-monorepo",
    "start": "tsc --watch --preserveWatchOutput",
    "test": "jest"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johngeorgewright/gen-x/issues"
  },
  "homepage": "https://github.com/johngeorgewright/gen-x#readme",
  "devDependencies": {
    "@gen-x/core": "^2.0.1",
    "@semantic-release/commit-analyzer": "9.0.1",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.0",
    "@semantic-release/github": "8.0.1",
    "@semantic-release/release-notes-generator": "10.0.2",
    "@types/jest": "27.0.2",
    "jest": "26.6.3",
    "rimraf": "3.0.2",
    "semantic-release": "^18.0.0",
    "semantic-release-monorepo": "7.0.5",
    "ts-jest": "26.5.6",
    "typescript": "4.4.3"
  },
  "dependencies": {
    "tslib": "2.3.1"
  }
}
